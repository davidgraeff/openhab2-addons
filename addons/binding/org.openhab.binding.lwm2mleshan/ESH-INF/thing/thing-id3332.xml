   <thing-type id="3332">
      <supported-bridge-type-refs>
         <bridge-type-ref id="lwm2mBridgeThing"/>
      </supported-bridge-type-refs>
      <label>Direction</label>
      <description><![CDATA[This IPSO object is used to report the direction indicated by a compass, wind vane, or other directional indicator. The units of measure is plane angle degrees (ucum:deg).              ]]></description>
      <channels>
         <channel id="CompassDirection" typeId="5705"/>
         <channel id="MinMeasuredValue" typeId="5601"/>
         <channel id="MaxMeasuredValue" typeId="5602"/>
         <channel id="ResetMinandMaxMeasuredValues" typeId="5605"/>
         <channel id="ApplicationType" typeId="5750"/>
      </channels>
      <properties>
         <property name="ObjectID">3332</property>
         <property name="ObjectURN">urn:oma:lwm2m:ext:3332</property>
         <property name="MultipleInstances">Multiple</property>
         <property name="Mandatory">Optional</property>
      </properties>
   </thing-type>
</thing:thing-descriptions>
